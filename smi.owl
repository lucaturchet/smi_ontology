    <!-- http://purl.org/ontology/iomust/smi#converter_sample_rate -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ontology/iomust/smi#converter_sample_rate">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/ontology/iomust/smi#GestureSensorHandler"/>
                    <rdf:Description rdf:about="http://purl.org/ontology/iomust/smi#HapticEngine"/>
                    <rdf:Description rdf:about="http://purl.org/ontology/iomust/smi#SoundEngine"/>
                    <rdf:Description rdf:about="http://purl.org/ontology/studio/device/Converter"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>converter sample rate</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/iomust/smi#cutoff_frequency -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ontology/iomust/smi#cutoff_frequency">
        <rdfs:domain rdf:resource="http://purl.org/ontology/studio/sigproc/LowPassFilter"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>cutoff frequency</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/iomust/smi#filter_type -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ontology/iomust/smi#filter_type">
        <rdfs:domain rdf:resource="http://purl.org/ontology/studio/sigproc/LowPassFilter"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>filter type</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/iomust/smi#mapping_function_parameter -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ontology/iomust/smi#mapping_function_parameter">
        <rdfs:domain rdf:resource="http://purl.org/ontology/iomust/smi#MappingTrasform"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>A parameter of the mapping function (e.g., the exponential coefficient of an exponential function)</rdfs:comment>
        <rdfs:label>mapping function parameter</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/iomust/smi#mapping_function_type -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ontology/iomust/smi#mapping_function_type">
        <rdfs:domain rdf:resource="http://purl.org/ontology/iomust/smi#MappingTrasform"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>A mathematical function that can be e.g., linear, exponential, logarithmic</rdfs:comment>
        <rdfs:label>mapping function type</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/iomust/smi#midi_port -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ontology/iomust/smi#midi_port">
        <rdfs:domain rdf:resource="http://purl.org/ontology/mo/Signal"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:label>midi port</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/iomust/smi#name_plugin -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ontology/iomust/smi#name_plugin">
        <rdfs:domain rdf:resource="https://w3id.org/aufx/ontology/1.0#PlugIn"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:label xml:lang="en">name_plugin</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/iomust/smi#threshold -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ontology/iomust/smi#threshold">
        <rdfs:domain rdf:resource="http://purl.org/ontology/iomust/smi#GestureSensorHandler"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>threshold</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/iomust/smi#works_at_audio_sampling_rate -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ontology/iomust/smi#works_at_audio_sampling_rate">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/ontology/iomust/smi#HapticEngine"/>
                    <rdf:Description rdf:about="http://purl.org/ontology/iomust/smi#SoundEngine"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label xml:lang="en">works at audio sampling rate</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/iomust/smi#works_at_sensor_sampling_rate -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ontology/iomust/smi#works_at_sensor_sampling_rate">
