#################################################################
#    Object Properties
#################################################################

###  http://purl.org/ontology/iomust/smi#SMIcomponent
:SMIcomponent rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://purl.org/ontology/studio/device/component> ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( :SMIHardwareDevice
                                          :SMISoftwareDevice
                                        )
                          ] ;
              rdfs:range <http://purl.org/ontology/iomust/internet_of_things/SmartInstrument> .


###  http://purl.org/ontology/iomust/smi#SMIhosts
:SMIhosts rdf:type owl:ObjectProperty ;
          rdfs:domain :SoundEngine ;
          rdfs:range <http://purl.org/ontology/studio/multitrack#MultitrackProject> ;
          rdfs:label "SMI hosts"@en .


###  http://purl.org/ontology/iomust/smi#component_mic
:component_mic rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://purl.org/ontology/studio/device/component> ;
               rdfs:domain :Microphone_Transducer ;
               rdfs:range <http://purl.org/ontology/studio/microphone/Microphone> .


###  http://purl.org/ontology/iomust/smi#component_platform
:component_platform rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf <http://purl.org/ontology/studio/device/component> ;
                    rdfs:domain <http://www.w3.org/ns/sosa/Platform> ;
                    rdfs:range <http://purl.org/ontology/iomust/internet_of_things/Thing> .


###  http://purl.org/ontology/iomust/smi#executes
:executes rdf:type owl:ObjectProperty ;
          rdfs:domain :SingleBoardComputer ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( :GestureSensorHandler
                                     :HapticEngine
                                     :SoundEngine
                                     :VisualEngine
                                   )
                     ] ;
          rdfs:label "executes" .


###  http://purl.org/ontology/iomust/smi#implementation
:implementation rdf:type owl:ObjectProperty ;
                rdfs:domain <http://purl.org/ontology/studio/main/Transform> ;
                rdfs:range <https://w3id.org/aufx/ontology/1.0#SoftwareImplementation> .


###  http://purl.org/ontology/iomust/smi#mapping_implementation
:mapping_implementation rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :implementation ;
                        rdfs:domain :MappingTrasform ;
                        rdfs:range :MappingHandler ;
                        rdfs:comment "A SMI's MappingTransform is implemented by a SMI's MappingHandler. Following the convention of the Audio Effects Ontology, a Mapping Handler is a software component of a SMI, while a Mapping Transform is the method which transforms a signal which results from an Observation of a Sensor into a parameter of a plugin." ;
                        rdfs:label "mapping implementation" .


###  http://purl.org/ontology/iomust/smi#maps
:maps rdf:type owl:ObjectProperty ;
      rdfs:domain :MappingTrasform ;
      rdfs:range <https://w3id.org/aufx/ontology/1.0#Parameter> .


###  http://purl.org/ontology/iomust/smi#processes_sensor_signal
:processes_sensor_signal rdf:type owl:ObjectProperty ;
                         rdfs:domain :GestureSensorHandler ;
                         rdfs:range <http://purl.org/ontology/mo/Signal> ;
                         rdfs:comment "Both the SMI Audio Input Handler and the SMI Gesture Sensor Handler process the (simple) results of an Observation" ;
                         rdfs:label "processes sensor signal"@en .


###  http://purl.org/ontology/iomust/smi#sensor_handler_implementation
:sensor_handler_implementation rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf :implementation ;
                               rdfs:domain :GestureSensorTransform ;
                               rdfs:range :GestureSensorHandler ;
                               rdfs:comment "Thus object property is used to express the relation betweem a GestureSensorHandler with a GestureSensorTransform" ;
                               rdfs:label "sensor handler implementation"@en .


###  http://purl.org/ontology/studio/device/component
<http://purl.org/ontology/studio/device/component> rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/sosa/hasResult
<http://www.w3.org/ns/sosa/hasResult> rdf:type owl:ObjectProperty ;
                                      rdfs:range <http://purl.org/ontology/mo/Signal> ;
                                      <http://schema.org/rangeIncludes> <http://purl.org/ontology/mo/Signal> ;
                                      rdfs:comment "We added Signal as a possible result of the SOSA \"hasResult\" object property. This is done to align the SOSA with the MusicOntology: this way we link the result of an Observation produced by a sensor to a Signal." .


###  https://w3id.org/aufx/ontology/1.0#implementation
<https://w3id.org/aufx/ontology/1.0#implementation> rdf:type owl:ObjectProperty ;
                                                    rdfs:subPropertyOf :implementation .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/ontology/iomust/smi#converter_sample_rate
:converter_sample_rate rdf:type owl:DatatypeProperty ;
                       rdfs:domain [ rdf:type owl:Class ;
                                     owl:unionOf ( :GestureSensorHandler
                                                   :HapticEngine
                                                   :SoundEngine
                                                   <http://purl.org/ontology/studio/device/Converter>
                                                 )
                                   ] ;
                       rdfs:range xsd:float ;
                       rdfs:label "converter sample rate" .


###  http://purl.org/ontology/iomust/smi#cutoff_frequency
:cutoff_frequency rdf:type owl:DatatypeProperty ;
                  rdfs:domain <http://purl.org/ontology/studio/sigproc/LowPassFilter> ;
                  rdfs:range xsd:float ;
                  rdfs:label "cutoff frequency" .


###  http://purl.org/ontology/iomust/smi#filter_type
:filter_type rdf:type owl:DatatypeProperty ;
             rdfs:domain <http://purl.org/ontology/studio/sigproc/LowPassFilter> ;
             rdfs:range xsd:string ;
             rdfs:label "filter type" .


###  http://purl.org/ontology/iomust/smi#has_name
:has_name rdf:type owl:DatatypeProperty ;
          rdfs:domain <https://w3id.org/aufx/ontology/1.0#PlugIn> ;
          rdfs:range rdfs:Literal ;
          rdfs:label "has name" .


###  http://purl.org/ontology/iomust/smi#mapping_function_parameter
:mapping_function_parameter rdf:type owl:DatatypeProperty ;
                            rdfs:domain :MappingTrasform ;
                            rdfs:range xsd:float ;
                            rdfs:comment "A parameter of the mapping function (e.g., the exponential coefficient of an exponential function)" ;
                            rdfs:label "mapping function parameter" .


###  http://purl.org/ontology/iomust/smi#mapping_function_type
:mapping_function_type rdf:type owl:DatatypeProperty ;
                       rdfs:domain :MappingTrasform ;
                       rdfs:range xsd:string ;
                       rdfs:comment "A mathematical function that can be e.g., linear, exponential, logarithmic" ;
                       rdfs:label "mapping function type" .


###  http://purl.org/ontology/iomust/smi#midi_port
:midi_port rdf:type owl:DatatypeProperty ;
           rdfs:domain <http://purl.org/ontology/mo/Signal> ;
           rdfs:range xsd:integer ;
           rdfs:label "midi port" .


###  http://purl.org/ontology/iomust/smi#threshold
:threshold rdf:type owl:DatatypeProperty ;
           rdfs:domain :GestureSensorHandler ;
           rdfs:range xsd:float ;
           rdfs:label "threshold" .


###  http://purl.org/ontology/iomust/smi#works_at_audio_sampling_rate
:works_at_audio_sampling_rate rdf:type owl:DatatypeProperty ;
                              rdfs:domain [ rdf:type owl:Class ;
                                            owl:unionOf ( :HapticEngine
                                                          :SoundEngine
                                                        )
                                          ] ;
                              rdfs:range xsd:float ;
                              rdfs:label "works at audio sampling rate"@en .


###  http://purl.org/ontology/iomust/smi#works_at_sensor_sampling_rate
:works_at_sensor_sampling_rate rdf:type owl:DatatypeProperty ;
                               rdfs:domain :GestureSensorHandler ;
                               rdfs:range xsd:float ;
                               rdfs:label "works at sensor sampling rate"@en .


#################################################################
#    Classes
#################################################################

###  http://purl.org/ontology/iomust/smiChannel
<http://purl.org/ontology/iomust/smiChannel> rdf:type owl:Class .


###  http://purl.org/ontology/iomust/internet_of_things/SmartInstrument
<http://purl.org/ontology/iomust/internet_of_things/SmartInstrument> rdf:type owl:Class .


###  http://purl.org/ontology/iomust/internet_of_things/Thing
<http://purl.org/ontology/iomust/internet_of_things/Thing> rdf:type owl:Class .


###  http://purl.org/ontology/iomust/smi#ADCaudio
:ADCaudio rdf:type owl:Class ;
          rdfs:subClassOf <http://purl.org/ontology/studio/sigproc/ADC> ;
          owl:disjointWith :ADCsensor ;
          rdfs:comment "Audio to Digital converter for audio signals" ;
          rdfs:label "ADC audio" .


###  http://purl.org/ontology/iomust/smi#ADCsensor
:ADCsensor rdf:type owl:Class ;
           rdfs:subClassOf <http://purl.org/ontology/studio/sigproc/ADC> ;
           rdfs:comment "Analog to Digital converter for gesture tracking sensors" ;
           rdfs:label "ADC sensor" .


###  http://purl.org/ontology/iomust/smi#Accelerometer
:Accelerometer rdf:type owl:Class ;
               rdfs:subClassOf :GestureTrackingSensor .


###  http://purl.org/ontology/iomust/smi#AcousticInstrument
:AcousticInstrument rdf:type owl:Class ;
                    rdfs:subClassOf <http://purl.org/ontology/mo/Instrument> ;
                    rdfs:label "Acoustic Instrument" .


###  http://purl.org/ontology/iomust/smi#AudioInputInterface
:AudioInputInterface rdf:type owl:Class ;
                     rdfs:subClassOf :SMIHardwareDevice .


###  http://purl.org/ontology/iomust/smi#AudioTrackingSensor
:AudioTrackingSensor rdf:type owl:Class ;
                     rdfs:subClassOf <http://www.w3.org/ns/sosa/Sensor> ;
                     rdfs:comment "A sensor that detects audio signals" .


###  http://purl.org/ontology/iomust/smi#DACactuator
:DACactuator rdf:type owl:Class ;
             rdfs:subClassOf <http://purl.org/ontology/studio/sigproc/DAC> ;
             owl:disjointWith :DACaudio ;
             rdfs:comment "Digital to Analog converter for haptic signals" .


###  http://purl.org/ontology/iomust/smi#DACaudio
:DACaudio rdf:type owl:Class ;
          rdfs:subClassOf <http://purl.org/ontology/studio/sigproc/DAC> ;
          rdfs:comment "Digital to Analog converter for audio signals" .


###  http://purl.org/ontology/iomust/smi#DistanceSensor
:DistanceSensor rdf:type owl:Class ;
                rdfs:subClassOf :GestureTrackingSensor .


###  http://purl.org/ontology/iomust/smi#ElectricInstrument
:ElectricInstrument rdf:type owl:Class ;
                    rdfs:subClassOf <http://purl.org/ontology/mo/Instrument> ;
                    rdfs:label "Electric Instrument" .


###  http://purl.org/ontology/iomust/smi#ElectroAcousticInstrument
:ElectroAcousticInstrument rdf:type owl:Class ;
                           rdfs:subClassOf <http://purl.org/ontology/mo/Instrument> ;
                           rdfs:label "Electro Acoustic Instrument" .


###  http://purl.org/ontology/iomust/smi#EmbeddedLoudspeakerSystem
:EmbeddedLoudspeakerSystem rdf:type owl:Class ;
                           rdfs:subClassOf :SoundDeliverySystem .


###  http://purl.org/ontology/iomust/smi#EmbeddedVibrationSpeakerSystem
:EmbeddedVibrationSpeakerSystem rdf:type owl:Class ;
                                rdfs:subClassOf :SoundDeliverySystem .


###  http://purl.org/ontology/iomust/smi#GestureInterface
:GestureInterface rdf:type owl:Class ;
                  rdfs:subClassOf :SMIHardwareDevice .


###  http://purl.org/ontology/iomust/smi#GestureSensorHandler
:GestureSensorHandler rdf:type owl:Class ;
                      rdfs:subClassOf :SMISoftwareDevice ;
                      rdfs:comment "A piece of software which carries out observations through sensors (like a microphone or a pressure sensor). Typically an SMI sensor observer acts upon the digital values of an ADC (analogue to digital converter)."@en ;
                      rdfs:label "Gesture Sensor Handler" .


###  http://purl.org/ontology/iomust/smi#GestureSensorTransform
:GestureSensorTransform rdf:type owl:Class ;
                        rdfs:subClassOf :SMITransform ;
                        owl:disjointWith :MappingTrasform ;
                        rdfs:comment "A transformation applied by a SMI Gesture Sensor Handler to the signal observed by a sensor tracking a player's gesture"@en ;
                        rdfs:label "Gesture Sensor Transform"@en .


###  http://purl.org/ontology/iomust/smi#GestureTrackingSensor
:GestureTrackingSensor rdf:type owl:Class ;
                       rdfs:subClassOf <http://www.w3.org/ns/sosa/Sensor> ;
                       rdfs:comment "A sensor that detects the performer's gestures" .


###  http://purl.org/ontology/iomust/smi#Gyroscope
:Gyroscope rdf:type owl:Class ;
           rdfs:subClassOf :GestureTrackingSensor .


###  http://purl.org/ontology/iomust/smi#HapticDeliverySystem
:HapticDeliverySystem rdf:type owl:Class ;
                      rdfs:subClassOf :SMIHardwareDevice .


###  http://purl.org/ontology/iomust/smi#HapticEngine
:HapticEngine rdf:type owl:Class ;
              rdfs:subClassOf :SMISoftwareDevice ;
              rdfs:comment "A piece of software that handles haptic processing" ;
              rdfs:label "Haptic Engine" .


###  http://purl.org/ontology/iomust/smi#InertialMeasurementUnit
:InertialMeasurementUnit rdf:type owl:Class ;
                         rdfs:subClassOf :GestureTrackingSensor .


###  http://purl.org/ontology/iomust/smi#Magnetometer
:Magnetometer rdf:type owl:Class ;
              rdfs:subClassOf :GestureTrackingSensor .


###  http://purl.org/ontology/iomust/smi#MappingHandler
:MappingHandler rdf:type owl:Class ;
                rdfs:subClassOf :SMISoftwareDevice ;
                rdfs:comment "A piece of software that handles the mapping functions from sensor values to plugin parameters" ;
                rdfs:label "Mapping Handler" .


###  http://purl.org/ontology/iomust/smi#MappingTrasform
:MappingTrasform rdf:type owl:Class ;
                 rdfs:subClassOf :SMITransform ;
                 rdfs:comment "A MappingTransform is the function which transforms a signal which results from an Observation of a Sensor into a parameter of a plugin." ;
                 rdfs:label "Mapping Transform"@en .


###  http://purl.org/ontology/iomust/smi#Microphone_Transducer
:Microphone_Transducer rdf:type owl:Class ;
                       rdfs:subClassOf :AudioTrackingSensor ;
                       rdfs:comment "The sensor embedded in a microphone" .


###  http://purl.org/ontology/iomust/smi#Potentiometer
:Potentiometer rdf:type owl:Class ;
               rdfs:subClassOf :GestureTrackingSensor .


###  http://purl.org/ontology/iomust/smi#PressureSensors
:PressureSensors rdf:type owl:Class ;
                 rdfs:subClassOf :GestureTrackingSensor .


###  http://purl.org/ontology/iomust/smi#PushButton
:PushButton rdf:type owl:Class ;
            rdfs:subClassOf :GestureTrackingSensor .


###  http://purl.org/ontology/iomust/smi#RE
:RE rdf:type owl:Class ;
    rdfs:subClassOf <https://w3id.org/aufx/ontology/1.0#PlugInAPI> ;
    rdfs:comment "Rack Extensions for a plugin API (by Propellerheads)"@en .


###  http://purl.org/ontology/iomust/smi#RibbonSensor
:RibbonSensor rdf:type owl:Class ;
              rdfs:subClassOf :GestureTrackingSensor .


###  http://purl.org/ontology/iomust/smi#SMIElectricAcousticIntrument
:SMIElectricAcousticIntrument rdf:type owl:Class ;
                              owl:equivalentClass [ owl:intersectionOf ( <http://purl.org/ontology/iomust/internet_of_things/SmartInstrument>
                                                                         :ElectroAcousticInstrument
                                                                       ) ;
                                                    rdf:type owl:Class
                                                  ] ;
                              rdfs:subClassOf <http://purl.org/ontology/iomust/internet_of_things/SmartInstrument> ;
                              rdfs:label "SMI Electric Acoustic Instrument" .


###  http://purl.org/ontology/iomust/smi#SMIElectricInstrument
:SMIElectricInstrument rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( <http://purl.org/ontology/iomust/internet_of_things/SmartInstrument>
                                                                  :ElectricInstrument
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       rdfs:subClassOf <http://purl.org/ontology/iomust/internet_of_things/SmartInstrument> ;
                       rdfs:label "SMI Electric Instrument" .


###  http://purl.org/ontology/iomust/smi#SMIHardwareDevice
:SMIHardwareDevice rdf:type owl:Class ;
                   rdfs:subClassOf <http://purl.org/ontology/studio/device/HardwareDevice> ;
                   rdfs:comment "Any hardware components of a smart instrument" .


###  http://purl.org/ontology/iomust/smi#SMISoftwareDevice
:SMISoftwareDevice rdf:type owl:Class ;
                   rdfs:subClassOf <http://purl.org/ontology/studio/device/SoftwareDevice> ;
                   rdfs:comment "Any software component of a smart musical instrument"@en ;
                   rdfs:label "SMI Software Device" .


###  http://purl.org/ontology/iomust/smi#SMITransform
:SMITransform rdf:type owl:Class ;
              rdfs:subClassOf <http://purl.org/ontology/studio/main/Transform> ;
              rdfs:label "SMI Transform"@en .


###  http://purl.org/ontology/iomust/smi#SMIVirtualRealityMusicalInstrument
:SMIVirtualRealityMusicalInstrument rdf:type owl:Class ;
                                    owl:equivalentClass [ owl:intersectionOf ( <http://purl.org/ontology/iomust/internet_of_things/SmartInstrument>
                                                                               :VirtualRealityMusicalInstrument
                                                                             ) ;
                                                          rdf:type owl:Class
                                                        ] ;
                                    rdfs:subClassOf <http://purl.org/ontology/iomust/internet_of_things/SmartInstrument> ;
                                    rdfs:label "SMI Virtual Reality Musical Instrument" .


###  http://purl.org/ontology/iomust/smi#SingleBoardComputer
:SingleBoardComputer rdf:type owl:Class ;
                     rdfs:subClassOf :SMIHardwareDevice ,
                                     <http://www.w3.org/ns/sosa/Platform> ;
                     rdfs:comment "The SMI Single Board Computer is an hardware component which is embedded into a SMI and runs software devices, including the SMI Sound Engine" ;
                     rdfs:label "Single Board Computer" .


###  http://purl.org/ontology/iomust/smi#SoftwareMultitrackProject
:SoftwareMultitrackProject rdf:type owl:Class ;
                           rdfs:subClassOf <http://purl.org/ontology/studio/device/SoftwareDevice> ;
                           rdfs:label "Software Multitrack Project"@en .


###  http://purl.org/ontology/iomust/smi#SoundDeliverySystem
:SoundDeliverySystem rdf:type owl:Class ;
                     rdfs:subClassOf :SMIHardwareDevice .


###  http://purl.org/ontology/iomust/smi#SoundEngine
:SoundEngine rdf:type owl:Class ;
             rdfs:subClassOf :SMISoftwareDevice ;
             rdfs:comment "A piece of software that handles audio processing" ;
             rdfs:label "Sound Engine" .


###  http://purl.org/ontology/iomust/smi#VST2
:VST2 rdf:type owl:Class ;
      rdfs:subClassOf <https://w3id.org/aufx/ontology/1.0#VST> ;
      owl:disjointWith :VST3 ;
      rdfs:label "VST 2"@en .


###  http://purl.org/ontology/iomust/smi#VST3
:VST3 rdf:type owl:Class ;
      rdfs:subClassOf <https://w3id.org/aufx/ontology/1.0#VST> ;
      rdfs:label "VST 3"@en .


###  http://purl.org/ontology/iomust/smi#VirtualRealityMusicalInstrument
:VirtualRealityMusicalInstrument rdf:type owl:Class ;
                                 rdfs:subClassOf <http://purl.org/ontology/mo/Instrument> ;
                                 rdfs:label "Virtual Reality Musical Instrument" .


###  http://purl.org/ontology/iomust/smi#VisualDeliverySystem
:VisualDeliverySystem rdf:type owl:Class ;
                      rdfs:subClassOf :SMIHardwareDevice .


###  http://purl.org/ontology/iomust/smi#VisualEngine
:VisualEngine rdf:type owl:Class ;
              rdfs:subClassOf :SMISoftwareDevice ;
              rdfs:comment "A piece of software that handles visual processing" ;
              rdfs:label "Visual Engine" .


###  http://purl.org/ontology/iomust/smi#multichannel
:multichannel rdf:type owl:Class ;
              rdfs:subClassOf <http://purl.org/ontology/iomust/smiChannel> .


