        <owl:disjointWith rdf:resource="http://purl.org/ontology/iomust/smi#MappingTrasform"/>
        <rdfs:comment xml:lang="en">A transformation applied by a SMI Gesture Sensor Handler to the signal observed by a sensor tracking a player&apos;s gesture</rdfs:comment>
        <rdfs:label xml:lang="en">Gesture Sensor Transform</rdfs:label>
    </owl:Class>
    


