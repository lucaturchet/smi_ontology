    <!-- http://purl.org/ontology/iomust/smi#GestureSensorHandler -->


    <owl:Class rdf:about="http://purl.org/ontology/iomust/smi#GestureSensorHandler">
        <rdfs:subClassOf rdf:resource="http://purl.org/ontology/iomust/smi#SMISoftwareDevice"/>
        <rdfs:comment xml:lang="en">A piece of software which carries out observations through sensors (like a microphone or a pressure sensor). Typically an SMI sensor observer acts upon the digital values of an ADC (analogue to digital converter).</rdfs:comment>
        <rdfs:label>Gesture Sensor Handler</rdfs:label>
    </owl:Class>
    


