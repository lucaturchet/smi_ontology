        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:label>has name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/iomust/smi#mapping_function_parameter -->


    <owl:DatatypeProperty rdf:about="http://purl.org/ontology/iomust/smi#mapping_function_parameter">
        <rdfs:domain rdf:resource="http://purl.org/ontology/iomust/smi#MappingTrasform"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>A parameter of the mapping function (e.g., the exponential coefficient of an exponential function)</rdfs:comment>
        <rdfs:label>mapping function parameter</rdfs:label>
