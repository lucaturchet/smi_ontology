    <!-- http://purl.org/ontology/iomust/smi#SMIcomponent -->


    <owl:ObjectProperty rdf:about="http://purl.org/ontology/iomust/smi#SMIcomponent">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/studio/device/component"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/ontology/iomust/smi#SMIHardwareDevice"/>
                    <rdf:Description rdf:about="http://purl.org/ontology/iomust/smi#SMISoftwareDevice"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://purl.org/ontology/iomust/internet_of_things/SmartInstrument"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/iomust/smi#SMIhosts -->


    <owl:ObjectProperty rdf:about="http://purl.org/ontology/iomust/smi#SMIhosts">
        <rdfs:domain rdf:resource="http://purl.org/ontology/iomust/smi#SoundEngine"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/studio/multitrack#MultitrackProject"/>
        <rdfs:label xml:lang="en">SMI hosts</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/iomust/smi#component_mic -->


    <owl:ObjectProperty rdf:about="http://purl.org/ontology/iomust/smi#component_mic">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/studio/device/component"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/iomust/smi#Microphone_Transducer"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/studio/microphone/Microphone"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/iomust/smi#component_platform -->


    <owl:ObjectProperty rdf:about="http://purl.org/ontology/iomust/smi#component_platform">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/studio/device/component"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/sosa/Platform"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/iomust/internet_of_things/Thing"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/iomust/smi#executes -->


    <owl:ObjectProperty rdf:about="http://purl.org/ontology/iomust/smi#executes">
        <rdfs:domain rdf:resource="http://purl.org/ontology/iomust/smi#SingleBoardComputer"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/ontology/iomust/smi#GestureSensorHandler"/>
                    <rdf:Description rdf:about="http://purl.org/ontology/iomust/smi#HapticEngine"/>
                    <rdf:Description rdf:about="http://purl.org/ontology/iomust/smi#SoundEngine"/>
                    <rdf:Description rdf:about="http://purl.org/ontology/iomust/smi#VisualEngine"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label>executes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/iomust/smi#implementation -->


    <owl:ObjectProperty rdf:about="http://purl.org/ontology/iomust/smi#implementation">
        <rdfs:domain rdf:resource="http://purl.org/ontology/studio/main/Transform"/>
        <rdfs:range rdf:resource="https://w3id.org/aufx/ontology/1.0#SoftwareImplementation"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/iomust/smi#mapping_implementation -->


    <owl:ObjectProperty rdf:about="http://purl.org/ontology/iomust/smi#mapping_implementation">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/iomust/smi#implementation"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/iomust/smi#MappingTrasform"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/iomust/smi#MappingHandler"/>
        <rdfs:comment>A SMI&apos;s MappingTransform is implemented by a SMI&apos;s MappingHandler. Following the convention of the Audio Effects Ontology, a Mapping Handler is a software component of a SMI, while a Mapping Transform is the method which transforms a signal which results from an Observation of a Sensor into a parameter of a plugin.</rdfs:comment>
        <rdfs:label>mapping implementation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/iomust/smi#maps -->


    <owl:ObjectProperty rdf:about="http://purl.org/ontology/iomust/smi#maps">
        <rdfs:domain rdf:resource="http://purl.org/ontology/iomust/smi#MappingTrasform"/>
        <rdfs:range rdf:resource="https://w3id.org/aufx/ontology/1.0#Parameter"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/iomust/smi#processes_sensor_signal -->


    <owl:ObjectProperty rdf:about="http://purl.org/ontology/iomust/smi#processes_sensor_signal">
        <rdfs:domain rdf:resource="http://purl.org/ontology/iomust/smi#GestureSensorHandler"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/mo/Signal"/>
        <rdfs:comment>Both the SMI Audio Input Handler and the SMI Gesture Sensor Handler process the (simple) results of an Observation</rdfs:comment>
        <rdfs:label xml:lang="en">processes sensor signal</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/iomust/smi#sensor_handler_implementation -->


    <owl:ObjectProperty rdf:about="http://purl.org/ontology/iomust/smi#sensor_handler_implementation">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/iomust/smi#implementation"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/iomust/smi#GestureSensorTransform"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/iomust/smi#GestureSensorHandler"/>
        <rdfs:comment>Thus object property is used to express the relation betweem a GestureSensorHandler with a GestureSensorTransform</rdfs:comment>
        <rdfs:label xml:lang="en">sensor handler implementation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/studio/device/component -->


    <owl:ObjectProperty rdf:about="http://purl.org/ontology/studio/device/component"/>
    


    <!-- http://www.w3.org/ns/sosa/hasResult -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/sosa/hasResult">
        <rdfs:range rdf:resource="http://purl.org/ontology/mo/Signal"/>
        <schema:rangeIncludes rdf:resource="http://purl.org/ontology/mo/Signal"/>
        <rdfs:comment>We added Signal as a possible result of the SOSA &quot;hasResult&quot; object property. This is done to align the SOSA with the MusicOntology: this way we link the result of an Observation produced by a sensor to a Signal.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/aufx/ontology/1.0#implementation -->


    <owl:ObjectProperty rdf:about="https://w3id.org/aufx/ontology/1.0#implementation">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/iomust/smi#implementation"/>
    </owl:ObjectProperty>
    


